{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "MasnoonDuas",
        "destination": "/MasnoonDuas.html"
      },
      {
        "source": "QuranDuas",
        "destination": "/QuranDuas.html"
      },
      {
        "source": "**",
        "destination": "/404.html"
      }
    ]
  }
}
